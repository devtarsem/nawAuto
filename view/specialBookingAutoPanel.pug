doctype html 
html(lang='en')
    head 
        meta(charset='utf-8')
        meta(http-equiv='X-UA-Compatible' content='IE=edge')
        title NAW &mdash; travel makes easy
        meta(name='description' content='naw is travel solving website in nitj it solves the problem of travel by providing the online auto booking')
        link(rel='icon' href='/img/logo.png')
        
        meta(name='viewport' content='width=device-width, initial-scale=1')
        link(rel='stylesheet' href='/css/index.css')
        script(type='module' src='https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js')
        script(nomodule src='https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js')

    body 
        nav.booking-nav.flex-2.pad16
            a(href='/') 
                img.logo(src='/img/logo404.png' alt='logo of company')
            .nav-container.flex-1.gap32 
                p.greet hi! tarsem singh 
                a.booing-btns(href='/') Home
                a.booing-btns(href='#') Logout 
                img.logo(src='/img/avatar6.png' alt='logo of company') 
        section.sectionSpecialAutoTrack.pad96 
            .special-container 
                .situation-container.flex-3.gap16.flex-dir
                    each order in orders 
                        .detail-container.flex-1.gap32.pad16 
                            img(src='/img/road.png' alt='customer-image').special-img
                            p.specialName= `${order.user.name}` 
                            p.specialRollNumber= `${order.user.rollNumber}`
                            p.specialArrivalPlace= `${order.arrivalPlace}`
                            p.crossSpecial &rarr;
                            p.specialDestinationPlace= `${order.destinationPlace}`
                            p.specialPhoneNumber= `${order.user.phoneNumber}`
                            p.specialAltPhoneNumber= `${order.user.AltphoneNumber}`
                        
                            p.specialFair 300/-
                            p.statusLable Accept
                            p.crossSpecial &rarr;
                            button.statusSpecialBtn.statusSpecialBtn--booking Accept now!

        script(src='/js/specialAutoBookingAccept.js')
        script(src='https://cdnjs.cloudflare.com/ajax/libs/axios/1.1.2/axios.min.js')

    footer.footer.pad96 
        img.logo(src='/img/logo404.png' alt='logo')
        .foot.grid.grid-4-col.gap96 
            .foot-navs.grid.grid-1-col.gap08 
                h4.fourth-heading policy 
                a.foot-link(href='#') Terms and conditions
                a.foot-link(href='#') Returns              
                a.foot-link(href='#') Refunds              
                a.foot-link(href='#') cancellation policy  
            .foot-navs.grid.grid-1-col.gap08 
                h4.fourth-heading company
                a.foot-link(href='/about-us') About founders
                a.foot-link(href='/new-service') carrear              
                a.foot-link(href='#') workers              
            .foot-navs.grid.grid-1-col.gap08 
                h4.fourth-heading social 
                a.foot-link(href='#') Twitter
                a.foot-link(href='#') Instagram              
                a.foot-link(href='#') Facebook
                a.foot-link(href='/feedback') Feedback 
            .foot-navs.grid.grid-1-col.gap08 
                h4.fourth-heading Special
                a.foot-link(href='/new-service') offers
                a.foot-link(href='/register-auto') Register your auto             
                a.foot-link(href='/new-service') Want work
        .api-publication.flex-1.gap96.pad24 
            p.api-label want to use our application Api
            button.api-doc-btn Use our Api 
        .copyright-container
            p.copyright Copyright &copy; claim by TTS all rightd reserved
