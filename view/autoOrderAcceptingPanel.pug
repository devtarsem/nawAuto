                            .about-driver.grid.grid-1-col.gap16 
                                .upper-about-detail.flex-2
                                    .img-name-combo.flex-1.gap08 
                                        img.driver-img(src='/img/kammo.jpeg' alt='auto driver image')
                                        p.driver-name= `${el.autoDriver[0].name}` 
                                        p.periority.label= `stand number : ${el.priority}`
                                    p.auto-periority-number= `${el.autoNumber}`
                            
                                .lower-about-detail.flex-2
                                    .contact-numbers.flex-1.gap16 
                                        p.num-1= `${el.autoDriver[0].phoneNumber}`
                                        p.num-2= `${el.autoDriver[0].AltphoneNumber}`
