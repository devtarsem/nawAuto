                .all-orders.pad48 
                    h3.accepted-order-heading.accepted-order-heading--update Available Savari's
                    .all-orders-display-container.grid.grid-2-col.gap24 
                        each order in orders
                            .order-1.flex-3.flex-dir.gap16 
                                .order-upper-detail.flex-2
                                    .img-name-combo.flex-2.gap08 
                                        img.driver-img(src='/img/road.png' alt='auto driver image')
                                         
                                        p.customer-name.customerOrderAcceptingBtn= `${order.user.name}`
                                        //-- error in this process as well
                                        each savariAccepted in AllSavariaccepted
                                            if savariAccepted.general===true 
                                                p.customer-name.customerOrderAcceptingBtn Booked
                                            
                                    p.customer-contact= `${order.user.phoneNumber}`
                                .order-lower-detail.grid.grid-1-col.gap16 
                                    .user-trip-detail.flex-1.gap08 
                                        p.savari-arrival= `${order.arrivalPlace}`
                                        p.cross-connection &rarr;
                                        p.savari-destination= `${order.destinationPlace}` 
