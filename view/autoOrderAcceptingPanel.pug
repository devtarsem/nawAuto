        section.savariFromCity-section 
            h2.from-city-heading Savari in City
            .savari-from-city-container.pad96.grid.grid-3-col.gap48 
                each savari in citySavaris
                    .savariCity.pad16.grid.grid-1-col.gap16 
                        img.city-customer-img(src='/img/road.png' alt='customer image')
                        p.name-of-savari-from-city= `${savari.user.name}`
                        p.name-of-savari-from-city arrival &darr;

                        p.arrival-from-city= `${savari.arrival}` 
                        p.name-of-savari-from-city destination &darr;

                        p.destination-from-city= `${savari.destination}` 
                        p.noOfPerson-from-city= `Number of person :- ${savari.NumberOfPerson}` 

                        p.phoneNumber-city= `mobile :- ${savari.user.phoneNumber}`
                        p.fair-city fair :- 55/-
                        //- if savariAccepted.preBookCity==true 
                            button.acceptCitySavariBtn payment pending ...
                        //- else 
                        
                        button.acceptCitySavariBtn Accept now!

            button.goToRideBtn Go to ride

        section.dual-savari-up-down 
            h2.from-city-heading Dual bookings 
            .dual-savari-container.pad96.grid.grid-3-col.gap48
                each order in dualOrder
                    .savariCity-dual.pad16.grid.grid-1-col.gap16 
                        img.city-customer-img.centeral(src='/img/road.png' alt='customer image')
                        p.name-of-savari-from-city-dual.centeral= `${order.user.name}`
                        p.arrival-from-city-dual.centeral= `arrival place :- ${order.arrival}`
                        p.destination-from-city-dual.centeral= `destination place :- ${order.destination}`
                        p.noOfPerson-from-city-dual.centeral= `Number of person :- ${order.NumberOfPerson}`
                        p.arrivalTime-city-dual.centeral= `arrival time :- ${order.arrivalTime}`
                        p.BackTime-city-dual.centeral= `back time :- ${order.BackTime}`

                        
                        p.date-city-dual.centeral= `date :- ${order.arrivalDate}`

                        p.phoneNumber-city-dual.centeral= `Mobile :- ${order.user.phoneNumber}`
                        p.fair-city-dual.centeral fair :- 55/-
                        //- if savariAccepted.preBookCity==true 
                            button.acceptCitySavariBtn payment pending ...
                        //- else 
                        
                        button.acceptCitySavariBtn-dual Accept now!




